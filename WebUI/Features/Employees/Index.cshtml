@model IReadOnlyList<Northwind.Application.Employees.Queries.Index.Model>
<h1>Employees</h1>

<table class="table">
  <thead>
  <tr>
    <th>
      Id
    </th>
    <th>
      First Name
    </th>
    <th>
      Last Name
    </th>
  </tr>
  </thead>
  <tbody>
  
  @foreach (var item in Model)
  {
    <tr>
      <td>
        @item.Id
      </td>
      <td>
        @item.FirstName
      </td>
      <td>
        @item.LastName
      </td>
    </tr>
  }
  </tbody>
</table>
